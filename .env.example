# Environment Variables Template
# Copy this file to .env.local for local development

# Spotify Playlist URL (Optional)
# Get your embed URL from Spotify: Share > Embed Playlist
# Example: https://open.spotify.com/embed/playlist/37i9dQZF1DXcBWIGoYBM5M
VITE_SPOTIFY_PLAYLIST_URL=

# Analytics (Optional)
# VITE_GA_TRACKING_ID=
# VITE_PLAUSIBLE_DOMAIN=

# Feature Flags (Optional)
# Set to 'false' to disable features
VITE_ENABLE_MUSIC=true
VITE_ENABLE_COUNTDOWN=true
VITE_ENABLE_CURSOR_TRAIL=true
